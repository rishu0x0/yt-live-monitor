{
  "version": 2,
  "builds": [
    { "src": "next.config.mjs", "use": "@vercel/next" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/dashboard", "dest": "/dashboard" },
    { "src": "/", "dest": "/" }
  ],
  
  "build": {
    "outputDirectory": ".next"
  }

}
